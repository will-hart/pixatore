{
  "name": "@pixatore/server",
  "version": "0.1.0",
  "license": "MIT",
  "description": "npm init template for bootstraping an empty Colyseus project",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "docs:api": "yarn typedoc --theme docusaurus2 --out ../docs/docs/api/server --skipSidebar",
    "start": "ts-node src/index.ts --build --watch",
    "loadtest": "colyseus-loadtest loadtest/example.ts --room my_room --numClients 2"
  },
  "author": "William Hart",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "0.13.1",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.7",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "typedoc": "^0.17.8",
    "typedoc-plugin-markdown": "^2.3.1"
  },
  "dependencies": {
    "@colyseus/monitor": "^0.12.2",
    "@colyseus/social": "^0.11.6",
    "@gamestdio/mathf": "^1.0.1",
    "@pixatore/shared": "0.1.0",
    "colyseus": "^0.14.0-alpha.10",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0"
  }
}
