{
  "name": "@pixatore/shared",
  "version": "0.1.0",
  "license": "MIT",
  "author": "William Hart",
  "description": "npm init template for bootstraping an empty Colyseus project",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "scripts": {
    "test": "yarn jest",
    "build": "tsc -p tsconfig.json --resolveJsonModule",
    "build:watch": "tsc -w -p tsconfig.json --resolveJsonModule",
    "docs:api": "yarn typedoc --theme docusaurus2 --out ../docs/docs/api/shared --skipSidebar",
    "start": "tsc -p tsconfig.json --resolveJsonModule --watch"
  },
  "dependencies": {
    "@colyseus/schema": "^1.0.0-alpha.28",
    "ts-bus": "2.3.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.7",
    "jest": "26.1.0",
    "ts-jest": "26.1.3",
    "typedoc": "^0.17.8",
    "typedoc-plugin-markdown": "^2.3.1"
  }
}
