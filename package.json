{
  "name": "tauri-game",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "app:serve": "yarn workspace @tauri-game/app serve",
    "app:build": "yarn workspace @tauri-game/app build",
    "app:test:unit": "yarn workspace @tauri-game/app test:unit",
    "app:lint": "yarn workspace @tauri-game/app lint",
    "app:tauri:build": "yarn workspace @tauri-game/app tauri:build",
    "app:dev": "yarn workspace @tauri-game/app dev",
    "server:dev": "yarn  workspace @tauri-game/server start",
    "server:loadtest": "yarn  workspace @tauri-game/server loadtest",
    "dev": "yarn run-p -nr server:dev app:dev",
    "predev": "yarn workspace @tauri-game/shared build"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5"
  }
}
